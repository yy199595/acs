{
    "/watch/start" : {
        "type": "POST",
        "async": false,
        "auth" : false,
        "desc": "启动一个进程",
        "bind": "Watch.StartProcess",
        "content-type": "application/json",
        "request": {
            "exe": "",
            "args": ""
        }
    },
    "/watch/close" : {
        "type": "GET",
        "async": false,
        "auth" : false,
        "desc": "启动一个进程",
        "bind": "Watch.CloseProcess",
        "request": {
            "pid": 0
        }
    },
    "/watch/list" : {
        "type": "GET",
        "async": false,
        "auth" : false,
        "desc": "获取进程列表",
        "bind": "Watch.List"
    }
}