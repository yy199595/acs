        "Auth": true,
        "Permiss": 20,
        "Desc": "分页查看账号",
        "Bind": "AccountManager.List",
        "ContentType": "application/json",
        "Request": {
            "page": 1
        }
    },
    "/account_mgr/find": {
        "Type": "GET",
        "Async": true,
        "Auth": true,
        "Permiss": 20,
        "Desc": "查看用户数据",
        "Bind": "AccountManager.Find",
        "Request": {
            "id": 1122
        }
    },
    "/account_mgr/change": {
        "Type": "POST",
        "Async": true,
        "Auth": true,
        "Permiss": 100,
