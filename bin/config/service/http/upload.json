{
    "/upload/oss": {
        "type": "GET",
        "auth": true,
        "bind": "FileUpload.Oss",
        "content-type": "multipart/form-data",
        "desc": "获取一个30秒的oss上传凭证",
        "request": {
            "type": 1
        }
    },
    "/upload/code": {
        "type": "POST",
        "auth": true,
        "bind": "FileUpload.Code",
        "content-type": "multipart/form-data",
        "desc": "上传微信二维码"
    },
    "/upload/club": {
        "type": "POST",
        "auth": true,
        "bind": "FileUpload.Club",
        "content-type": "multipart/form-data",
        "desc": "上传俱乐部头像"
    },
    "/upload/icon": {
        "type": "POST",
        "auth": true,
        "bind": "FileUpload.Icon",
        "content-type": "multipart/form-data",
        "desc": "上传用户头像"
    },
    "/upload/file": {
        "type": "POST",
        "auth": false,
        "permission": 5,
        "bind": "FileUpload.File",
        "content-type": "multipart/form-data",
        "desc": "上传活动图片资源等"
    }
}