{
    "/table_mgr/find": {
        "type": "POST",
        "async": true,
        "auth": true,
        "permission": 100,
        "desc": "分页查找Mongo表",
        "bind": "Collection.FindPage",
        "content-type": "application/json",
        "request": {
            "tab": "xxx",
            "page": 1,
            "filter" : { }
        }
    },
    "/table_mgr/delete": {
        "type": "GET",
        "async": true,
        "auth": true,
        "permission": 100,
        "desc": "删除集合",
        "Record" : true,
        "bind": "Collection.Delete",
        "request": {
            "tab": "xxx"
        }
    },
    "/table_mgr/list": {
        "type": "GET",
        "async": true,
        "auth": true,
        "permission": 100,
        "desc": "查看所有集合",
        "bind": "Collection.List"
    },
    "/table_mgr/tables": {
        "type": "GET",
        "async": true,
        "auth": true,
        "permission": 100,
        "desc": "查看所有库和表",
        "bind": "Collection.Tables"
    },
    "/table_mgr/export": {
        "type": "GET",
        "async": true,
        "auth": false,
        "desc": "导出一个表",
        "bind": "MongoMgr.Export",
        "request" : {
            "tab" : "xx.xx"
        }
    }
}