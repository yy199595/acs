{
    "log": [
        {
            "open" : false,
            "name" : "debug",
            "level" : "debug"
        },
        {
            "open" : true,
            "name" : "info",
            "level" : "info",
            "max_line": 10240,
            "max_size": 104448,
            "path": "${WORK_DIR}/log"
        },
        {
            "open" : true,
            "name" : "warn",
            "level" : "warn",
            "max_line": 10240,
            "max_size": 104448,
            "path": "${WORK_DIR}/log"
        },
        {
            "open" : true,
            "name" : "error",
            "level" : "error",
            "max_line": 10240,
            "max_size": 104448,
            "path": "${WORK_DIR}/log",
            "pem" : "${WORK_DIR}/ssl/ca.pem",
            "wx" : "",
            "ding" : ""
        },
        {
            "open" : true,
            "name" : "fatal",
            "level" : "fatal",
            "max_line": 10240,
            "max_size": 104448,
            "path": "${WORK_DIR}/log",
            "pem" : "${WORK_DIR}/ssl/ca.pem",
            "wx" : "",
            "ding" : ""
        }
    ],
    "log_dir" : "${WORK_DIR}/log"
}