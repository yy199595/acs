{
	"include": [
		"com/db.json",
		"com/wx.json",
		"com/com.json",
		"com/log.json",
		"com/backup.json"
	],
	"core": {
		"name": "${name}"
	},
	"lua": {
		"main": "${lua}",
		"modules" : [
			"RegistryComponent"
		]
	},
	"listen": {
		"gate": {
			"address": "tcp://127.0.0.1:${gate}",
			"component" : "OuterWebSocketComponent"
		}
	},
	"http": {
		"auth": false,
		"index": "index.html",
		"domain": "https://www.huwai.pro",
		"root": "${WORK_DIR}/www/dist",
		"upload": "${WORK_DIR}/www/dist",
		"download": "${WORK_DIR}/www/dist",
		"header": {
			"Access-Control-Allow-Origin": "*"
		},
		"pool" : 100,
		"send_timeout" : 5,
		"read_timeout" : 5,
		"range-name" : "https://www.mocklib.com/mock/random/name"
	},
	"path": {
		"node": "${WORK_DIR}/config/node/all.json",
		"machine": "${WORK_DIR}/config/run/node/machine.json"
	}
}