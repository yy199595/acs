{
	"include": [
		"com/db.json",
		"com/com.json",
		"com/log.json"
	],
	"core": {
		"name": "${name}"
	},
	"lua": {
		"main": "${lua}"
	},
	"listen": {
		"http": {
			"port": "${http}",
			"ip": "127.0.0.1"
		},
		"rpc": {
			"port": "${rpc}",
			"ip": "127.0.0.1"

		},
		"gate": {
			"port": "${gate}",
			"ip": "127.0.0.1"
		},
		"udp" : {
			"port" : "${udp}",
			"ip": "127.0.0.1"
		},
		"kcp" : {
			"port" : "${kcp}",
			"ip": "127.0.0.1"
		}
	},
	"master": {
		"host": "http://127.0.0.1:8088"
	},
	"http": {
		"auth": true,
		"index": "index.html",
		"domain": "https://www.huwai.pro",
		"root": "${WORK_DIR}/www/dist",
		"upload": "${WORK_DIR}/www/dist",
		"download": "${WORK_DIR}/www/dist",
		"header": {
			"Access-Control-Allow-Origin": "*"
		}
	},
	"path": {
		"machine": "",
		"home": "${WORK_DIR}/config/card/home.json",
		"cluster": "${WORK_DIR}/config/cluster/debug.json"
	}
}